{
  "name": "alti",
  "version": "0.5.1",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev:server": "nodemon ./server/bin/www | bunyan --output short --time local",
    "dev:client": "webpack --config webpack.dev.js --watch",
    "start": "node ./server/bin/www",
    "start-dev": "run-p dev:*"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/preset-react",
      "@instructure/ui-babel-preset"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{html,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --cache --fix"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "server"
    ]
  },
  "dependencies": {
    "@instructure/canvas-theme": "^7.1.1",
    "@instructure/ui": "^7.1.1",
    "@instructure/ui-babel-preset": "^7.1.1",
    "@instructure/ui-webpack-config": "^7.1.1",
    "@sentry/node": "^5.18.0",
    "bunyan": "^1.8.13",
    "buzzapi": "^1.3.10",
    "canvas-lms-api": "^0.2.3",
    "cookie-parser": "~1.4.5",
    "debug": "~4.1.0",
    "dotenv": "^8.2.0",
    "ejs": "~3.1.3",
    "express": "~4.17.1",
    "express-jwt": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "http-errors": "~1.8.0",
    "ims-lti": "^3.0.2",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "parse-link-header": "^1.0.1",
    "passport": "^0.4.1",
    "passport-lti": "^0.0.7",
    "prop-types": "^15.6.2",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "superagent": "^5.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.5.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "husky": ">=4",
    "lint-staged": ">=10",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "semver": "^7.3.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.0.9"
  }
}
